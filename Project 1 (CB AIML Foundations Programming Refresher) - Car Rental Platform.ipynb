    "car_rates = {\n",
    "    #for these lists, first entry is cars in stock, second is hourly rate, third is daily rate, fourth is weekly rate\n",
    "    'raycaster': ['raycaster', 5, 22, 91, 275],\n",
    "    'venom': ['venom', 3, 15, 74, 161],\n",
    "    'zenith': ['zenith', 8, 17, 80, 213]\n",
    "}"
